{
  "name": "netsvelte",
  "version": "0.0.1",
  "scripts": {
    "prestart": "node aspnetcore-https && node aspnetcore-svelte",
    "start": "rimraf ./build && rimraf ./.svelte-kit && svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest src --config jest.config.json",
    "test:watch": "npm run test -- --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.0",
    "@sveltejs/adapter-static": "^1.0.0-next.26",
    "@sveltejs/kit": "next",
    "@testing-library/jest-dom": "^5.14.0",
    "@testing-library/svelte": "^3.0.0",
    "@types/bootstrap": "^5.1.9",
    "@types/jest": "^27.0.0",
    "@types/testing-library__jest-dom": "^5.14.0",
    "babel-jest": "^27.0.0",
    "bootstrap": "^5.1.3",
    "cross-env": "^7.0.3",
    "jest": "^27.0.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-jester": "^2.0.1",
    "svelte-preprocess": "^4.9.4",
    "ts-jest": "^27.0.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "type": "module"
}
